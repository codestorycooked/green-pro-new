@model IEnumerable<GreenPro.AdminInterface.ViewModels.GarageTeamModel> 
@{
    ViewBag.Title = "TeamList";
}

<h2>Team List</h2>
<p>
    @Html.ActionLink("Create New", "CreateOrEditTeam")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            Garage Name 
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Title)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Active)
        </th>
        <th>
            Created On
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GarageName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOn)
                
            </td>
           
            <td>
                @Html.ActionLink("Edit", "CreateOrEditTeam", new { id = item.Id }) 
               
            </td>
        </tr>
    }

</table>


