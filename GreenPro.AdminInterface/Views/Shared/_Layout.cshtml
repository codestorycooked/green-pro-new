<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Welcome to GreenPRO Admin</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    <div id="wrapper">
        @*Navigation*@
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Green Pro Admin</a>
            </div>
            <!-- /.navbar-header -->
            @Html.Partial("_LoginPartial")

            <!-- /.navbar-top-links -->
            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">

                        <li>
                            @Html.ActionLink("Dashboard", "Index", "Home")
                        </li>
                        <li>
                            @Html.ActionLink("Work Management", "Index", "CrewAdmin")
                            @*<a href="#">Work Management<span class="fa arrow"></span></a>*@
                            @*<ul class="nav nav-second-level">
                                    <li>@Html.ActionLink("Manage Car Types", "Index", "Cartypes")</li>
                                    <li>@Html.ActionLink("Manage Garages", "Index", "Garages")</li>
                                </ul>*@
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            @Html.ActionLink("Unassigned Cars", "Index", "UnAssignedCars")
                        </li>
                        <li>
                            <a href="#">Garages<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                @*<li>@Html.ActionLink("Manage Car Types", "Index", "Cartypes")</li>*@
                                <li>@Html.ActionLink("Manage Garages", "Index", "Garages")</li>
                                <li>@Html.ActionLink("Cars", "Index", "Car")</li>
                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="#">Packages<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>@Html.ActionLink("Manage Package Services", "Index", "Services")</li>
                                <li>@Html.ActionLink("Manage Packages", "Index", "Packages")</li>

                                <li>@Html.ActionLink("User Packages", "UserPackageList", "Packages")</li>

                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="#">Payments<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>@Html.ActionLink("Manage Tax", "Index", "Taxes")</li>
                                <li>@Html.ActionLink("Manage Paypal Payments", "Index", "Taxes")</li>
                                <li>@Html.ActionLink("Upcoming Renewals", "Index", "Taxes")</li>
                                <li>@Html.ActionLink("Manage Renewals", "Index", "Taxes")</li>

                            </ul>
                            <!-- /.nav-second-level -->
                        </li>

                        <li>
                            <a href="#">User Management<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>@Html.ActionLink("Manage Users", "Index", "UsersAdmin")</li>
                                <li>@Html.ActionLink("Manage Roles", "Index", "RolesAdmin")</li>

                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                        <li>
                            <a href="#">Settings<span class="fa arrow"></span></a>
                            <ul class="nav nav-second-level">
                                <li>@Html.ActionLink("Manage Teams", "TeamList", "Setting")</li>
                                <li>@Html.ActionLink("Default Days Setting", "Index", "Setting")</li>
                                <li>@Html.ActionLink("Manage States", "Index", "States")</li>
                                <li>@Html.ActionLink("Manage Cities", "Index", "StateCity")</li>
                                <li>@Html.ActionLink("Car Sides", "Index", "LogDetailCarSides")</li>


                            </ul>
                            <!-- /.nav-second-level -->
                        </li>
                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>
            <!-- /.navbar-static-side -->
        </nav>

        <div id="page-wrapper">

            @Html.Partial("Notifications")
            <div>
                @RenderBody()
            </div>
            
        </div>
    </div>



    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")

    @RenderSection("scripts", required: false)

</body>
</html>
